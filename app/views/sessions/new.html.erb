<h1>Log in</h1>
<%= form_tag sessions_path do %>
  <div class="field">
    <%= label_tag :email_address %>
    <br/>
    <%= text_field_tag :email_address %>
  </div>
  <div class="field">
    <%= label_tag :password %>
    <br/>
    <%= password_field_tag :password %>
  </div>
  <div class="actions">
    <%= submit_tag :Login %>
  </div>
<% end %>
<%= link_to 'Sign up!', new_user_path %>
